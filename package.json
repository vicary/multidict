{
  "name": "multidict",
  "version": "1.0.6",
  "description": "MultiDict is a TypeScript implementation of a key-value store that supports multiple keys and values.",
  "main": "./dist/MultiDict.js",
  "module": "./dist/MultiDict.mjs",
  "types": "./dist/MultiDict.d.ts",
  "typings": "./dist/MultiDict.d.ts",
  "exports": {
    ".": {
      "types": "./dist/MultiDict.d.ts",
      "import": "./dist/MultiDict.mjs",
      "require": "./dist/MultiDict.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "repository": "https://github.com/vicary/multidict.git",
  "author": "Vicary A. <vicary.archangel@member.mensa.org>",
  "license": "MIT",
  "keywords": [
    "dictionary",
    "map",
    "object map",
    "array map",
    "graph",
    "two-way association",
    "bidirectional mapping",
    "many to many relationship",
    "multiple keys",
    "multiple values"
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/vicary"
  },
  "packageManager": "yarn@3.5.0",
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "jsxSingleQuote": false,
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "scripts": {
    "build": "node build.js && tsc",
    "prepack": "yarn build"
  },
  "devDependencies": {
    "esbuild": "^0.17.12",
    "typescript": "^5.0.2"
  }
}
