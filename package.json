{
  "name": "polymap",
  "version": "1.0.0",
  "description": "PolyMap is a TypeScript implementation of a key-value store that supports multiple keys and values.",
  "main": "dist/PolyMap.js",
  "module": "dist/PolyMap.mjs",
  "exports": {
    ".": {
      "import": "./dist/PolyMap.mjs",
      "require": "./dist/PolyMap.js"
    },
    "./package.json": "./package.json"
  },
  "repository": "https://github.com/vicary/polymap.git",
  "author": "Vicary A. <vicary.archangel@member.mensa.org>",
  "license": "MIT",
  "packageManager": "yarn@3.5.0",
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "jsxSingleQuote": false,
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "scripts": {
    "build": "node build.js && tsc"
  },
  "devDependencies": {
    "esbuild": "^0.17.12",
    "typescript": "^5.0.2"
  }
}
